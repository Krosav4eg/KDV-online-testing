<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Footer regression tests" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="C34351">
        <classes>
            <class name="testcases.footer.FooterTest">
                <methods>
                    <include name="verifyAboutShopLinkTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C34358">
        <classes>
            <class name="testcases.footer.FooterTest">
                <methods>
                    <include name="verifyCustomerPickupLinkTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>