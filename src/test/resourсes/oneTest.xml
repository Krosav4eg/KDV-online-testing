<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="User Access To Pages And Functions Of The System" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="First Test">
        <classes>
            <class name="regressionSuit.FirstTest"/>
        </classes>
    </test>
    <test name="Sec Test">
        <classes>
            <class name="regressionSuit.SecondTest"/>
        </classes>
    </test>
</suite>