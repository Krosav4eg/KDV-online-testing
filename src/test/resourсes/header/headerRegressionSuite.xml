<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Header regression tests" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="C34282">
        <classes>
            <class name="testcases.header.HeaderTest">
                <methods>
                    <include name="verifyLogoTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C34283">
        <classes>
            <class name="testcases.header.HeaderTest">
                <methods>
                    <include name="verifyChangingCityTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C34287">
        <classes>
            <class name="testcases.header.HeaderTest">
                <methods>
                    <include name="verifyClosingModalWindowTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>