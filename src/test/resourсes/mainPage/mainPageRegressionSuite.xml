<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Main Page Regression Tests" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="C34321">
        <classes>
            <class name="testcases.MainPage.MainPageTest">
                <methods>
                    <include name="verifyLowerPriceTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C34322">
        <classes>
            <class name="testcases.MainPage.MainPageTest">
                <methods>
                    <include name="verifyClosingLowerPriceTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C34323">
        <classes>
            <class name="testcases.MainPage.MainPageTest">
                <methods>
                    <include name="verifyingAboutLinkLowerPrice"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C34324">
        <classes>
            <class name="testcases.MainPage.MainPageTest">
                <methods>
                    <include name="verifyOpeningFreeDeliveryTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>