<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Smm Block Regression Tests" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="C37058">
        <classes>
            <class name="testcases.mainPage.blockSmm.BlockSmmTest">
                <methods>
                    <include name="verifyOpeningVkLinkTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C37062">
        <classes>
            <class name="testcases.mainPage.blockSmm.BlockSmmTest">
                <methods>
                    <include name="verifyOpeningInstagramLinkTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C37063">
        <classes>
            <class name="testcases.mainPage.blockSmm.BlockSmmTest">
                <methods>
                    <include name="verifyOpeningGooglePlayLinkTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
