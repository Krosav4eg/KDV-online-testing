<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Header Regression Tests" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="C34339">
        <classes>
            <class name="testcases.mainPage.hitOfSales.HitOfSalesTest">
                <methods>
                    <include name="verifyTotalSumHitSalesTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="C343340">
        <classes>
            <class name="testcases.mainPage.hitOfSales.HitOfSalesTest">
                <methods>
                    <include name="verifyAddingProductIntoBasketTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
