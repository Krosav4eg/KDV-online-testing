<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Site Tests">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="1">
        <classes>
            <class name="testcases.header.HeaderTest">
                <methods>
                    <include name="verifyPersonalCabinetLinkTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="2">
        <classes>
            <class name="testcases.header.HeaderTest">
                <methods>
                    <include name="verifyProductsInBasketTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>