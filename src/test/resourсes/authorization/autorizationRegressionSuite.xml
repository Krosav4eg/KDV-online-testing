<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Authorization regression tests">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="C34462-C34463,C34467">
        <classes>
            <class name="testcases.authorization.AuthorizationTest">
                <methods>
                    <include name="verifyTypingIncorrectPasswordInAuthTest"/>
                    <include name="verifyTypingCorrectLoginIncorrectPasswordInAuthTest"/>
                    <include name="verifyTypingEmptyDataInAuthTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>



