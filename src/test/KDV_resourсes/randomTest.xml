<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Site Tests" parallel="tests">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="listener.ListenerTest"/>
    </listeners>

    <test name="Ordering OrderingPageTest OrderingGuestPageTest">
        <classes>
            <class name="KDV_testcases.header.HeaderTest">
                <methods>
                    <include name="verifyMyBasketWithProductTest"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>